<h1>New student</h1>

<%= error_messages_for :student %>

<% form_for(@student) do |f| %>
  <p>
    <b>Studentid</b><br />
    <%= f.text_field :studentId %>
  </p>

  <p>
    <b>Surname</b><br />
    <%= f.text_field :surname %>
  </p>

  <p>
    <b>Firstname</b><br />
    <%= f.text_field :firstName %>
  </p>

  <p>
    <b>Email</b><br />
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', students_path %>
